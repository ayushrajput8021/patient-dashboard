{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"outDir": "./dist",
		"strict": true,
		"sourceMap": true,
		"declaration": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*", "prisma/**/*"],
	"exclude": ["node_modules", "**/*.test.ts"]
}
